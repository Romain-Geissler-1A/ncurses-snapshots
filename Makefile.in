# Master Makefile for ncurses library.

SHELL = /bin/sh

@SET_MAKE@

all \
clean \
distclean \
mostlyclean \
realclean \
install ::
	cd src;   $(MAKE) @nc_cv_makeflags@ $@
	cd man;   $(MAKE) @nc_cv_makeflags@ $@
	cd test;  $(MAKE) @nc_cv_makeflags@ $@
	cd c++;   $(MAKE) @nc_cv_makeflags@ $@

distclean ::
	rm -f config.cache config.log config.status Makefile include/config.h
